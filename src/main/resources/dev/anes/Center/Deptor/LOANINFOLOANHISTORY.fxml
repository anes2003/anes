<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<BorderPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.ozz.layout.Center.Deptor.LoanInfoHistoryController">
   <top>
      <HBox prefHeight="3.0" prefWidth="1140.0" BorderPane.alignment="CENTER">
         <children>
            <FontIcon fx:id="leftarrowIcon" iconLiteral="fas-arrow-left" iconSize="33" styleClass="red" wrappingWidth="31.875">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </FontIcon>
            <HBox prefHeight="33.0" prefWidth="31.0" />
            <VBox>
               <children>
                  <Button fx:id="loanButton" mnemonicParsing="false" prefHeight="63.0" prefWidth="202.0" text="Loan">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
               </children>
            </VBox>
            <VBox>
               <children>
                  <Button fx:id="infoButton" mnemonicParsing="false" prefHeight="61.0" prefWidth="202.0" text="Info">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
               </children>
            </VBox>
            <VBox>
               <children>
                  <Button fx:id="historyButton" mnemonicParsing="false" prefHeight="65.0" prefWidth="202.0" text="Loan History">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </top>
   <center>
      <StackPane fx:id="stackPane" prefHeight="457.0" prefWidth="1140.0" styleClass="background2">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </StackPane>
   </center>
</BorderPane>
